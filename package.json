{
  "name": "fitness-tracker",
  "version": "1.0.0",
  "description": "",
  "main": "src/server.js",
  "scripts": {
    "clean": "rimraf dist/*  ",
    "test": "mocha --exit src/api/tests/*.test.js --timeout 10000",
    "test-coverage": "nyc npm run test",
    "dev:client": "parcel src/views/home.html --no-source-maps --port 3000",
    "dev:server": "nodemon --inspect src/server",
    "build:client": "parcel build src/views/home.html --out-dir dist",
    "build:server": "NODE_ENV=production babel src -s -d dist",
    "production": "concurrently -n \"Clean, Build-client, Build-server\" \"npm run clean\" \"npm run build:client\" \"npm run build:server\"",
    "start": "node bin/production"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "child_process": "^1.0.2",
    "concurrently": "^4.1.2",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "http-status": "^1.3.2",
    "method-override": "^3.0.0",
    "underscore": "^1.9.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "mocha": "^6.2.0",
    "morgan": "^1.9.1",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1",
    "parcel-bundler": "^1.12.3",
    "rimraf": "^3.0.0"
  }
}
